public class ExternApp {

	public static void main(String args[]) throws Exception {
	
		Runtime runtime = Runtime.getRuntime();
		//open notepad
		runtime.exec("netepad.exe");
		
		//open chrome
		runtime.exec("C:\\Program Files (x86)\\Google\\Chrome\\Application\\chrome.exe");
		
		//path or any other application example: media player
		runtime.exec("C:\\Program Files (x86)\\Windows Media Player\\wmplayer.exe");
	}
}
